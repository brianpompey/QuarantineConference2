<%= form_with model: @session do |f| %>

    <div class="form-group">
    <%= f.label :email, "Email:" %>
    <br>
    <%= f.text_field :email, class: "form-control" %>
    </div>

    <div class="form-group">
    <%= f.label :password, "Password:" %>
    <br>
    <%= f.password_field :password, class: "form-control" %>
    </div>

    <div class="actions">
    <%= f.submit class:"btn btn-primary" %>
    </div>

<% end %>